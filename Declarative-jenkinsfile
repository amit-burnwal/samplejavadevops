pipeline{
 agent any
 environment{
   path = "/usr/share/maven/bin:$PATH"
         }
    stages{
     
     stage("Github checkout"){
	  steps{
        git credentialsId: 'github', url: 'https://github.com/amit-burnwal/samplejavadevops'
            }
            }
           
     stage('Build'){
      steps{
       sh 'mvn clean package'
          }
         }
       }
     }
